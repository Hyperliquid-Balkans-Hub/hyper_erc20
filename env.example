# Private key for deploying contracts (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Optional: Token configuration
TOKEN_NAME=MyToken
TOKEN_SYMBOL=MTK
TOTAL_SUPPLY=1000000
TOKEN_DECIMALS=18

# Optional: Initial minting configuration
# MINT_PERCENTAGE: Percentage of total supply to mint initially (1-100)
# If not set, 100% will be minted to deployer during deployment
MINT_PERCENTAGE=100

# Optional: HyperSwap V2 Liquidity Addition
# ADD_LIQUIDITY: Set to true to automatically add liquidity after deployment
ADD_LIQUIDITY=false

# Liquidity configuration (only used if ADD_LIQUIDITY=true)
# LIQUIDITY_TOKEN_AMOUNT: Amount of your token to add to liquidity pool
LIQUIDITY_TOKEN_AMOUNT=10000

# LIQUIDITY_HYPE_AMOUNT: Amount of HYPE to pair with your token (in HYPE units)
LIQUIDITY_HYPE_AMOUNT=1

# LIQUIDITY_SLIPPAGE: Slippage tolerance in percentage (1-50)
LIQUIDITY_SLIPPAGE=5 